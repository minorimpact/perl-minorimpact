[% INCLUDE header %]

<!-- TEMPLATE:default.search -->
[% IF search && objects.keys.size %]
    <table class=list>
    [% FOREACH type IN objects.keys %]
        <tr>
            <td class=fieldname valign=top>
                [% typeName(type) %]
            </td>
            <td valign=top>
                [% INCLUDE list_object objects = objects.$type %]
            </td>
        </tr>
    [% END %]
    </table>
[% ELSE %]
    No results
[% END %]

